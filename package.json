{
  "name": "apex-loads-fe",
  "version": "1.0.",
  "private": true,
  "scripts": {
    "dev": "run-s generate-colors-ts dev:server",
    "dev:server": "next dev -p 3000",
    "build": "next build",
    "start": "next start -p 3000",
    "openapi-generate": "npx openapi-typescript@6.7.6 https://stageapi.carscout.ai/api/documentation/json -o ./src/shared/api/schema/v1.d.ts --header 'Authorization: Basic ZGV2OmRvY3NfZm9yX0RFVl8hISFfOA==' --enum",
    "generate-colors-ts": "node ./src/core/scripts/css-hsl-vars-to-colors-in-ts.js ./src/core/styles/colors.scss ./src/core/styles/tailwind/app.colors.ts",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "lint": "next lint",
    "prepare": "husky",
    "type-check": "npx tsc --noEmit",
    "analyze": "ANALYZE=true next build",
    "postinstall": "npm run prepare"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.61.5",
    "clsx": "^2.1.1",
    "dayjs": "^1.11.13",
    "lodash-es": "^4.17.21",
    "next": "^14.2.18",
    "openapi-fetch": "^0.11.0",
    "react": "^18",
    "react-dom": "^18",
    "server-only": "^0.0.1",
    "tailwind-merge": "^2.5.2",
    "valibot": "^0.41.0"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^14.2.7",
    "@svgr/core": "^8.1.0",
    "@svgr/plugin-jsx": "^8.1.0",
    "@tanstack/react-query-devtools": "^5.61.5",
    "@types/lint-staged": "^13.3.0",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@typescript-eslint/eslint-plugin": "^8.3.0",
    "@typescript-eslint/parser": "^8.3.0",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.2.7",
    "eslint-config-prettier": "^9.1.0",
    "husky": "^9.1.5",
    "lint-staged": "^15.2.10",
    "npm-run-all2": "^6.2.2",
    "postcss": "^8",
    "prettier": "^3.3.3",
    "sass": "^1.77.8",
    "tailwindcss": "^3.4.1",
    "typescript": "^5",
    "unplugin-auto-import": "^0.18.2",
    "unplugin-icons": "^0.20.2"
  }
}
